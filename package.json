{
  "name": "nan-ui",
  "version": "0.1.0",
  "description": "A modern React component library with beautiful UI components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "preview": "vite preview",
    "clean": "rm -rf dist coverage storybook-static",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write ."
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "library",
    "typescript",
    "css",
    "form-components"
  ],
  "author": "Bruno Alves",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brunonnalves/nan-ui.git"
  },
  "bugs": {
    "url": "https://github.com/brunonnalves/nan-ui/issues"
  },
  "homepage": "https://github.com/brunonnalves/nan-ui#readme",
  "peerDependencies": {
    "react": ">=19.1.1",
    "react-dom": ">=19.1.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.8.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-storybook": "^9.1.4",
    "husky": "^8.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
